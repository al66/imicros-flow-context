{
  "name": "imicros-flow-context",
  "version": "0.0.5",
  "description": "Moleculer persistent context store",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --coverage --no-cache --silent",
    "test-local": "jest --coverage --no-cache --config=local.js --silent",
    "test-context": "jest test/context.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-flow-context.git"
  },
  "keywords": [
    "Moleculer",
    "persistent",
    "context"
  ],
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al66/imicros-flow-context/issues"
  },
  "homepage": "https://github.com/al66/imicros-flow-context#readme",
  "devDependencies": {
    "jest": "^27.3.1",
    "moleculer": "^0.14.18",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "cassandra-driver": "^4.6.3",
    "imicros-acl": "^0.0.12"
  }
}
